# find qt dependencies package
find_package(@SINGLEAPPLICATION_QT_FINDPKG_CMDSTR@)

@PACKAGE_INIT@

set(SINGLEAPPLICATION_VERSION_MAJOR "@SINGLEAPPLICATION_VERSION_MAJOR@")
set(SINGLEAPPLICATION_VERSION_MINOR "@SINGLEAPPLICATION_VERSION_MINOR@")
set(SINGLEAPPLICATION_VERSION_PATCH "@SINGLEAPPLICATION_VERSION_PATCH@")

set_and_check(SINGLEAPPLICATION_INCLUDE_DIRS "@PACKAGE_INCLUDE_INSTALL_DIRS@")

include("${CMAKE_CURRENT_LIST_DIR}/@PROJECT_NAME@-targets.cmake")

# Make sure we find packages for our dependencies
foreach(_PKG IN ITEMS @SINGLEAPPLICATION_PKGS@)
    find_package(${_PKG} REQUIRED)
endforeach ()

set(SINGLEAPPLICATION_LIBRARY @PROJECT_NAME@::@PROJECT_NAME@)

check_required_components(@PROJECT_NAME@)
